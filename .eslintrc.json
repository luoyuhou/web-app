{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [ "warn", 2 ],
    "no-unused-vars": 0,
    "accessor-pairs": 2,
    "no-multiple-empty-lines": [1, {"max": 1}],
    "semi":["error","always"],
    "no-trailing-spaces": 2,
    "require-atomic-updates": "off",
    "quotes": "error"
  }
}
